        case 109: break;
        case 23: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "R_RES")); return new Symbol(sym.R_RES, yycolumn, yyline, yytext());
          }
        case 110: break;
        case 2: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "ENTERO")); return new Symbol(sym.ENTERO, yycolumn, yyline, yytext());
          }
        case 111: break;
        case 5: 
          { /* No hace nada, solo ignora los comentarios de una línea */
          }
        case 112: break;
        case 42: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "R_CONSOLE")); return new Symbol(sym.R_CONSOLE, yycolumn, yyline, yytext());
          }
        case 113: break;
        case 25: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "R_MIN")); return new Symbol(sym.R_MIN, yycolumn, yyline, yytext());
          }
        case 114: break;
        case 48: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "R_VARIANZA")); return new Symbol(sym.R_VARIANZA, yycolumn, yyline, yytext());
          }
        case 115: break;
        case 20: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "DECIMAL")); return new Symbol(sym.DECIMAL, yycolumn, yyline, yytext());
          }
        case 116: break;
        case 1: 
          { System.out.println("Error Lexico: " + yytext() + " | Fila:" + yyline + " | Columna: " + yycolumn); errores.add(new Errores("Error Lexico: "+yytext(), yyline, yycolumn));
          }
        case 117: break;
        case 9: 
          { System.out.println("Token: " + yytext() + " Linea: " + yyline + " Columna: " + yycolumn); tokens.add(new Token(yytext(), yyline, yycolumn, "POR")); return new Symbol(sym.POR, yycolumn, yyline, yytext());
          }
        case 118: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            zzDoEOF();
              { return new java_cup.runtime.Symbol(sym.EOF); }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
